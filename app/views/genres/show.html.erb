<!DOCTYPE html>
<html>
  <head>
    <title><%= @genre.name %></title>
  </head>
  <body>
    Artists:<br>

    <% @genre.artists.each do |artist| %>
        <a href ="./tmp/_site/artists/<%= artist.to_slug %>.html"><%= artist.name %></a>
        <br>
      <% end %>
      <br><br>

  Songs: <br>
    <% @genre.songs.each do |song| %>
        <a href="./tmp/_site/songs/<%= song.to_slug %>.html"><%= song.artist.name %> - <%= song.name %> - <%= song.genre.name %></a>
        <br>
      <% end %>
  </body>
</html>